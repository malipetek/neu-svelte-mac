{
  "name": "neutralino-svelte-macos-app",
  "version": "1.0.0",
  "private": true,
  "pkg": {
    "scripts": "pkg_nodejs/*.js",
    "targets": [
      "nodev12-macos-x64"
    ],
    "outputPath": "pkg_dist"
  },
  "bin": "pkg_nodejs/prices-csv-fixer.js",
  "scripts": {
    "build": "node build-app.js",
    "dev": "rollup -c -w",
    "start": "neu run",
    "build_nexe": "nexe --input=./nexe_nodejs/prices-csv-fixer.js --output=./nexe_dist/csv-fix target=macos-14.15.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@types/execa": "^2.0.0",
    "execa": "^5.1.1",
    "fs-extra": "^10.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "@shockpkg/icon-encoder": "^1.3.1",
    "rollup-plugin-copy": "^3.4.0",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "@types/jsonexport": "^3.0.2",
    "csv": "^5.5.3",
    "jsonexport": "^3.2.0",
    "sirv-cli": "^1.0.0",
    "svelte-json-tree": "^0.1.0"
  }
}
